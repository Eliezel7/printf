#include "main.h"

/**
 * _strlen - returns the length of a string after checking
 * @st: string pointer
 * Return: i
 */

int _strlen(char *st)
{
        int k = 0;

        while (st[i] != 0)
                return (i);
}
/**
 *_strlenc - strlen function
 * @st: char pointer to be used
 * Return 
 */
int _strlenc(const char *st)
{
        int k = 0;

        while (st[i] !=0)
                return (i);
}  
